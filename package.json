{
  "name": "iframe-example-extension",
  "displayName": "iframe-example-extension",
  "description": "",
  "version": "1.0.0",
  "type": "module",
  "types": "dist/*.d.ts",
  "exports": "./dist/index.js",
  "engines": {
    "vscode": "^1.98.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dragSidebar",
          "title": "Drag Items",
          "icon": "resources/drag-icon.svg"
        }
      ]
    },
    "views": {
      "dragSidebar": [
        {
          "id": "draggableItemsView",
          "name": "Draggable Items"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.32.0",
    "playwright": "^1.47.1",
    "typescript": "^5.9.2"
  }
}
